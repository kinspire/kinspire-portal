{
    "name": "kinspire-portal",
    "version": "0.1.0",
    "dependencies": {
        "@progress/kendo-drawing": "^1.6.0",
        "@progress/kendo-react-pdf": "^3.4.1",
        "bootstrap": "^4.2.1",
        "classnames": "^2.2.6",
        "history": "^4.7.2",
        "moment": "^2.22.2",
        "react": "^16.5.2",
        "react-bootstrap": "^1.0.0-beta.5",
        "react-dom": "^16.5.2",
        "react-navbar": "^3.0.0",
        "react-router-dom": "^4.3.1",
        "reactstrap": "^8.0.1",
        "semantic-ui-react": "^0.87.1",
        "sweetalert": "^2.1.2"
    },
    "scripts": {
        "build-css": "node-sass-chokidar src/ -o src/",
        "build-js": "react-scripts build",
        "build": "npm-run-all build-css build-js",
        "deploy": "npm-run-all build fb-deploy",
        "eject": "react-scripts eject",
        "electron": "electron .",
        "fb-deploy": "firebase deploy",
        "flow": "flow",
        "serve": "serve -s build",
        "start-js": "cross-env PORT=3001 react-scripts start",
        "start": "npm-run-all -p watch-css start-js",
        "test": "react-scripts test --env=jsdom",
        "watch-css": "yarn run build-css && node-sass-chokidar src/ -o src/ --watch --recursive"
    },
    "devDependencies": {
        "cross-env": "^5.2.0",
        "electron": "^3.0.5",
        "flow-bin": "^0.75.0",
        "node-sass-chokidar": "^1.3.3",
        "npm-run-all": "^4.1.3",
        "react-scripts": "^1.1.5",
        "serve": "^7.0.0"
    },
    "repository": "git://github.com/kinspire/kinspire-portal.git",
    "license": "MIT",
    "main": "src/electron-main.js"
}
