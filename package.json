{
    "name": "kinspire-portal",
    "version": "0.1.0",
    "dependencies": {
        "bootstrap": "^4.1.3",
        "classnames": "^2.2.6",
        "firebase": "^5.5.2",
        "history": "^4.7.2",
        "moment": "^2.22.2",
        "react": "^16.5.2",
        "react-dom": "^16.5.2",
        "react-router-dom": "^4.3.1",
        "sweetalert": "^2.1.2"
    },
    "scripts": {
        "build-css": "node-sass-chokidar src/ -o src/",
        "build-js": "react-scripts build",
        "build": "npm-run-all build-css build-js",
        "deploy": "npm-run-all build serve",
        "eject": "react-scripts eject",
        "electron": "electron .",
        "flow": "flow",
        "serve": "serve -s build",
        "start-js": "cross-env PORT=3001 react-scripts start",
        "start": "npm-run-all -p watch-css start-js",
        "test": "react-scripts test --env=jsdom",
        "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive"
    },
    "comments": {
        "electron": "^2.0.0"
    },
    "devDependencies": {
        "cross-env": "^5.2.0",
        "electron": "^3.0.5",
        "firebase-tools": "^5.0.1",
        "flow-bin": "^0.75.0",
        "node-sass-chokidar": "^1.3.3",
        "npm-run-all": "^4.1.3",
        "react-scripts": "^1.1.5",
        "serve": "^7.0.0"
    },
    "repository": "git://github.com/kinspire/kinspire-portal.git",
    "license": "MIT",
    "main": "src/electron-main.js"
}
